[Desktop Entry]
Type=Application
Version=1.0
# Do not translate the word "RELEASE".  It is used as a marker by live.
_Name=Install RELEASE
_Comment=Install this system permanently to your hard disk
_Keywords=lingmo-installer;
#use sh because pkexec is broken under xfce/lxce http://pad.lv/1193526
Exec=sudo --preserve-env=DBUS_SESSION_BUS_ADDRESS,XDG_DATA_DIRS,XDG_RUNTIME_DIR,GTK_THEME sh -c 'lingmo-installer gtk_ui'
Icon=lingmo-installer
Terminal=false
Categories=GTK;System;Settings;
#X-Lingmo-Gettext-Domain=lingmo-installer-desktop
X-Ayatana-Appmenu-Show-Stubs=False
